<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zytc.web_3d_school.dao.mapper.UserMapper">
    <select id="CheckUser" resultType="com.zytc.web_3d_school.pojo.User">
        select * from sys_user where login_name=#{User.login_name} and login_pwd=#{User.login_pwd}
    </select>
</mapper>